<%request.setCharacterEncoding("utf-8");%>   
<%!
	int intCh(String s){
		int num = 0;
		if(s!=null && !s.equals("")){
			num = Integer.parseInt(s);
		}
		return num;
	}
	double dblCh(String s){
		double num = 0.0;
		if(s!=null && !s.equals("")){
			num = Double.parseDouble(s);
		}
		return num;
	}
%> 
